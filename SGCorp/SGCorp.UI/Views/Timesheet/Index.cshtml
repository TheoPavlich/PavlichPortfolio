@using System.Web.Mvc.Html
@model SGCorp.UI.Models.EmployeeTimesheetVM
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Timesheets: Select Employee</h2>

<div class="row">
    <div class="clo=lg-6">
        @using (Html.BeginForm("Index", "Timesheet"))
        {
            <div class="form-group">
                @Html.DropDownListFor(m=>m.Employee.EmpId, Model.Employees, new {@class = "form-control"})
            </div>
        }
    </div>
</div>

